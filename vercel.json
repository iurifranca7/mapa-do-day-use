{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/((?!api/|logo.svg|.*\\.(?:png|jpg|jpeg|gif|svg|ico|css|js)).*)",
      "destination": "/index.html"
    }
  ]
}